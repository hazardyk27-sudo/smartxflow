packages = ["cairo", "dbus", "ffmpeg-full", "fontconfig", "freetype", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "pkg-config", "qhull", "tcl", "tk", "xcodebuild", "xorg.libX11", "xorg.libXext", "xorg.libXrender", "xorg.libxcb", "xvfb-run", "zlib", "zstd"]

[workflows]
