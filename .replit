args = "SmartXFlow Web"

[[workflows.workflow]]
name = "SmartXFlow Test"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python test_scraper.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "SmartXFlow Web"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

